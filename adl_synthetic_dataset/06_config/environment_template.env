# ADL Synthetic Dataset Environment Variables
# Copy this file to .env and fill in your API keys

# Deepgram API (for voice generation)
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# OpenAI API (if regenerating data)
OPENAI_API_KEY=your_openai_api_key_here

# AWS (if using alternative voice services)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Dataset Configuration
MAX_SCENARIOS=120
VOICE_GENERATION_ENABLED=true
OUTPUT_DIRECTORY=03_outputs
